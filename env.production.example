# Production Environment Variables
# Copy this file to .env.production and fill in your values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Application URL (IMPORTANT: Set this to your production domain)
# This is used for password reset redirects and other email links
NEXT_PUBLIC_APP_URL=https://gms.hostado.net

# Optional: Cron Secret for scheduled tasks
CRON_SECRET=your_secure_random_string_here

# Node Environment
NODE_ENV=production

# Optional: Custom Port (if needed)
PORT=3000





